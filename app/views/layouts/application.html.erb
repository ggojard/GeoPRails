<!DOCTYPE html>
<html ng-app='GeoP' id='ng-app'>
	<head>
		<meta http-equiv="Content-type" content="text/html; charset=utf-8">
		<%= render '/layouts/ios' %>
		<title><%= !@global_company.nil? && @global_company.name%> / Surfy.pro / v0.7.35b</title>
		<%= include_gon %>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<script type="text/javascript" src="https://www.google.com/jsapi"></script>
		<%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
		<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
		<!-- <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.1.2/ui-bootstrap.min.js"></script> -->
		<%= csrf_meta_tags %>

	</head>
	<body id="body-container" ng-cloak>
		<div id="spinner-zone"></div>
		<div  ng-controller="BodyCtrl">
			<%= render 'layouts/notifications' %>
			<%= render 'layouts/right_popup' %>
			<%= render 'layouts/menu_top' %>
			<div class="container-fluid main-container">
				<%= yield %>
			</div>
		</div>
		<%= render 'layouts/analytics' %>
		<%= render 'layouts/ios_links' %>
	</body>
</html>
