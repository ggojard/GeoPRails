<h1>Login#login</h1>
<%= form_for(:admin_user, :url => session_path(:admin_user)) do |f| %>
  <%= f.text_field :email %>
  <%= f.password_field :password %>
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me %>
  <%= f.submit 'Sign in' %>
  <%= link_to "Forgot your password?", new_password_path(:admin_user) %>
<% end %>
